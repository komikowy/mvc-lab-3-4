<!-- views/List.ejs -->

<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>
<body>
    <%- include('partials/navigation') %>
    <h1><%= pageTitle %></h1>
    <ul>
        <% students.forEach(student => { %>
            <li>
                <p><%= student.fullName %></p>
                <p><%= student.code %></p>
                <p><%= student.fieldOfStudies %></p>
            </li>
        <% }); %>
    </ul>
</body>
</html>
